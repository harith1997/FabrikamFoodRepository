             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FabrikamFood.FindUsScreen">
  <ContentPage.Content>
    <StackLayout>
      <Label Text="find us screen" TextColor="#FEDCBA" VerticalOptions="Center" HorizontalOptions="Center" />

      <fragment xmlns:android="http://schemas.android.com/apk/res/android"
          android:id="@+id/map"
          android:layout_width="match_parent"
